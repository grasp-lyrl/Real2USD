{
  "matching": {
    "method": "greedy",
    "iou_mode": "obb",
    "frame_normalization": "gt_global_yaw",
    "iou_threshold": 0.1,
    "require_label_match": false,
    "range_buckets_m": [
      2.0,
      4.0
    ],
    "compute_open_set_metrics": true,
    "os_similarity_threshold": 0.9
  },
  "prediction": {
    "label_source": "prefer_retrieved_else_label",
    "ignore_raw_labels": [
      "courtyard",
      "lamp",
      "toy_car",
      "elevator",
      "beam",
      "home_interior",
      "wood_wall",
      "server_room",
      "ceiling",
      "scratch",
      "dormitory",
      "person",
      "television",
      "podium",
      "playroom"
    ],
    "ignore_raw_contains": [],
    "ignore_canonical_labels": []
  },
  "label_resolution": {
    "use_embedding_for_unresolved": true,
    "learn_new_aliases": true,
    "embedding_min_score": 0.5
  },
  "paths": {
    "canonical_labels": "./canonical_labels.json",
    "label_aliases": "./label_aliases.json",
    "learned_label_aliases": "./learned_label_aliases.json"
  }
}